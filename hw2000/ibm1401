--------IBM 1401---------	--Punch Card--
Print	Char 	Binary		Char	Code	EBCDIC
-----	-----	---------	----	------	---------
0	0	1 00 1010	0	0	1111 0000 ***
1	1	0 00 0001	1	1	1111 0001
2	2	0 00 0010	2	2	1111 0010
3	3	1 00 0011	3	3	1111 0011
4	4	0 00 0100	4	4	1111 0100
5	5	1 00 0101	5	5	1111 0101
6	6	1 00 0110	6	6	1111 0110
7	7	0 00 0111	7	7	1111 0111
8	8	0 00 1000	8	8	1111 1000
9	9	1 00 1001	9	9	1111 1001
				:	2-8	0111 1010
#	#	0 00 1011	#	3-8	0111 1011
@	@	1 00 1100	@	4-8	0111 1100
				'	5-8	0111 1101
				=	6-8	0111 1110
	check	1 00 1111	"	7-8	0111 1111 (tape mark)
blank	blank	1 00 0000	blank	space	0100 0000 ***
A	A	0 11 0001	A	12-1	1100 0001
B	B	0 11 0010	B	12-2	1100 0010
C	C	1 11 0011	C	12-3	1100 0011
D	D	0 11 0100	D	12-4	1100 0100
E	E	1 11 0101	E	12-5	1100 0101
F	F	1 11 0110	F	12-6	1100 0110
G	G	0 11 0111	G	12-7	1100 0111
H	H	0 11 1000	H	12-8	1100 1000
I	I	1 11 1001	I	12-9	1100 1001
&	?	1 11 1010		12-0			"plus zero"
				cent	12-2-8	0100 1010 ?
.	.	0 11 1011	.	12-3-8	0100 1011
lozenge	lozenge	1 11 1100	<	12-4-8	0100 1100
	(			(	12-5-8	0100 1101
	<			+	12-6-8	0100 1110
	tri	1 11 1111	|	12-7-8	0100 1111 (group mark)
&	&	1 11 0000	&	12	0101 0000 ***
J	J	1 10 0001	J	11-1	1101 0001
K	K	1 10 0010	K	11-2	1101 0010
L	L	0 10 0011	L	11-3	1101 0011
M	M	1 10 0100	M	11-4	1101 0100
N	N	0 10 0101	N	11-5	1101 0101
O	O	0 10 0110	O	11-6	1101 0110
P	P	1 10 0111	P	11-7	1101 0111
Q	Q	1 10 1000	Q	11-8	1101 1000
R	R	0 10 1001	R	11-9	1101 1001
-	!	0 10 1010		11-0			"minus zero"
				!	11-2-8	0101 1010
$	$	1 10 1011	$	11-3-8	0101 1011
*	*	0 10 1100	*	11-4-8	0101 1100
	)			)	11-5-8	0101 1101
	;			;	11-6-8	0101 1110
	delta			not	11-7-8	0101 1111 (mode change)
-	-	0 10 0000	-	11	0110 0000 ***
=	cent	0 01 0000
/	/	1 01 0001	/	0-1	0110 0001
S	S	1 01 0010	S	0-2	1110 0010
T	T	0 01 0011	T	0-3	1110 0011
U	U	1 01 0100	U	0-4	1110 0100
V	V	0 01 0101	V	0-5	1110 0101
W	W	0 01 0110	W	0-6	1110 0110
X	X	1 01 0111	X	0-7	1110 0111
Y	Y	1 01 1000	Y	0-8	1110 1000
Z	Z	0 01 1001	Z	0-9	1110 1001
ne	ne	0 01 1010	(none)	0-2-8	0110 1010 ? (record mark)
,	,	1 01 1011	,	0-3-8	0110 1011
%	%	0 01 1100	%	0-4-8	0110 1100
	=			_	0-5-8	0110 1101 (word sep)
	'			>	0-6-8	0110 1110
	"			?	0-7-8	0110 1111 (tape seg mark)
