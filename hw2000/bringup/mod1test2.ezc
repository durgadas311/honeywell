00000* MOD1 PROGRAM TO TEST READING DISK
00000         PROG  MOD1T2
00000         ORG   1340
00000         ADMODE4
00000         MAC   MOD1MSIO
00000*
00000L        MIOC  A
00000*
00000* FILE ITEM SIZE IS ASSUMED TO BE 80,
00000* AS IS TAPE BLOCK SIZE
00000*
00000C F1     MCA   A,
00000C        10    RECD2,DEVTAB,MOVE,RECD1
00000C        20    MYFILE01,
00000C        31    00,
00000C        40    ERROR,
00000C        43    DEXIT,
00000L        44    ERROR,
00000* ALL EXITS ARE FATAL, NEVER RETURN TO MIOC
00000*
00000  START  CAM   60
00000*
00000L        MSOPENF1,IN
00000*
00000L NEXT   MSGET F1,
00000         PDT   RECD1,12,02,01    PRINT RECORD
00000         PCB   *,12,02,00
00000         B     NEXT
00000*
00000  DEXITC DCW   #1B0
00000  DEXIT  SCR   ENDR,70
00000         BCE   DONE,DEXITC,01       EOF
00000         PDT   DAT,10,02,01
00000         PCB   *,10,02,00
00000         B     DONE
00000*
00000  ERRORC DCW   #1B0
00000  ERROR  SCR   ENDR,70
00000         PDT   ERR,10,02,01
00000         PCB   *,10,02,00
00000L DONE   MSCLOSF1,
00000         PDT   FIN,10,02,01
00000         PCB   *,10,02,00
00000         H
00000         NOP
00000*
00000  ENDR   DSA   0
00000*
00000  DEVTAB RESV  0
00000         DCW   #3C040000     PCA, LUN, 00
00000 L       DCW   #1B0
00000*
00000   RECD1 RESV, 80       ITEM BUFFER
00000 L       DCW   #1A
00000*
00000   RECD2 RESV, 256      BLOCK BUFFER
00000 L       DCW   #1A
00000 L       DCW   #1A
00000 L       DCW   #1A
00000*
00000 F ERR   DC    @ERRORX@
00000 F DAT   DC    @DATA ERRORX@
00000 F FIN   DC    @FINISHEDX@
00000*
00000         END   START
