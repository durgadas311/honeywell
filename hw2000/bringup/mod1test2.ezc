00000* MOD1 PROGRAM TO TEST READING DISK FILE
00000         PROG  MOD1T2
00000         ORG   1340
00000         ADMODE4
00000         MAC   MOD1MSIO
00000*
00000L        MIOC  A
00000*
00000* FILE ITEM SIZE IS ASSUMED TO BE 80,
00000* AS IS TAPE BLOCK SIZE
00000*
00000C F1     MCA   A,DEVTAB,
00000C        10    RECD2,,MOVE,RECD1
00000C        20    MYFILE01,
00000C        31    00,
00000L        43    DEXIT,
00000* LOOK FOR ERRORS ON CONSOLE TYPEWRITER
00000*
00000  START  CAM   60
00000*
00000L        MSOPENF1,IN
00000C        MUCA  F1,VOLNM,VNM,
00000C              VOLSN,VSN,
00000L              FNAM,FID
00000*
00000L NEXT   MSGET F1,
00000         PDT   RECD1,12,02,01    PRINT RECORD
00000         PCB   *,12,02,00
00000         B     NEXT
00000*
00000L DONE   MSCLOSF1,
00000         PDT   FIN,10,02,01
00000         PCB   *,10,02,00
00000         B     (164)      PROGRAM EXIT
00000*
00000  ABORT  DCW   #1C40
00000  DEXITC DCW   #1B0
00000  DEXIT  SCR   DXITZ,70
00000         BCE   DONE,DEXITC,01    EOF
00000         EXM   ABORT,DEXITC,01
00000   DXITZ B     0
00000*
00000  DEVTAB RESV  0
00000         DCW   #3C040000     PCA, LUN, 00
00000 L       DCW   #1B0
00000*
00000   RECD1 RESV, 80       ITEM BUFFER
00000 L       DCW   #1A
00000*
00000   RECD2 RESV, 256      BLOCK BUFFER
00000 L       DCW   #1A
00000 L       DCW   #1A
00000 L       DCW   #1A
00000*
00000   FIN   DC    @FINISHED @
00000  VOLNM  DC    #6A
00000         DC    @ @
00000  VOLSN  DC    #6A
00000         DC    @ @
00000  FNAM   DC    #10A
00000 L       DCW   #1B0
00000*
00000         END   START
