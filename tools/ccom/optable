/* int -> float */
cr51:
%aw,n
	movif	A1,R

%nw*,n
	F*
	movif	#1(R),R

%n,n
	F
	movif	R,R

/* float, double -> int */
cr52:
%nf,n
	F
	movfi	R,R

/* double (float) to long */
cr56:
%nf,n
	F
	setl
	dect	sp
	dect	sp
	movfi	R,(sp)
	mov	(sp)+,R
	mov	(sp)+,R+
	seti

/* long to double */
cr57:
%al,n
	setl
	movif	A1,R
	seti

%nl*,n
	F*
	setl
	movif	#1(R),R
	seti

%nl,n
	FS
	setl
	movif	(sp)+,R
	seti

/* unsigned long to float(double) */
cr127:
%aul,n
	mov	A1+,(sp)
	dect	sp
	mov	A1,(sp)
	bl	I
	c	(sp)+,(sp)+

%nul*,n
	F*
	mov	#1+2(R),(sp)
	dect	sp
	mov	#1(R),(sp)
	bl	I
	c	(sp)+,(sp)+

%nul,n
	FS
	bl	I
	c	(sp)+,(sp)+

/* integer to long */
cr58:
%eu,n
	F1!
	clr	R

%nu,n
	F
	mov	R,R1
	clr	R

%e,n
	F1!
	mov	R1,R
	sra	R,15

%n,n
	F
	mov	R,R1
	sra	R,15

/* long to integer */
cr59:
%al,n
%aul,n
	mov	A1+,R

%nl*,n
%nul*,n
	F*
	mov	#1+2(R),R

/* *, /, % for longs. */
cr82:
