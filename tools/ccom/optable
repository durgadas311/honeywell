	S1
	I	R1,R-

%n,n
	SS
	F
	T
	I	(sp)+,R-


/* /= and %= where divisor is unsigned or known to be positive */
cr119:
%aub,c
%a,c
	movB1	A1',R
	>1	R,8
	T
	li	r0,A2
	I	r0,R-
	<1	R=,8
	movB1	R=,A1
	>1	R=,8

%aub,aw
%a,aw
	movB1	A1',R
	>1	R,8
	T
	I	A2,R-
	<1	R=,8
	movB1	R=,A1
	>1	R=,8

%aub,n
%a,n
	SS
	movB1	A1',R
	>1	R,8
	T
	I	(sp)+,R-
	<1	R=,8
	movB1	R=,A1
	>1	R=,8

%eub*,n
%e*,n
	SS
	F1*
	movB1	#1(R1),R
	>1	R,8
	T
	I	(sp)+,R-
	<1	R=,8
	movB1	R=,#1(R1)
	>1	R=,8

%nub*,n
%n*,n
	FS*
	SS
	mov	@2(sp),r1
	movB1	#1(r1),R
	>1	R,8
	T
	I	(sp)+,R-
	<1	R=,8
	movB1	R=,#1(R1)
	>1	R=,8
	inct	sp

/* (int *) - (int *) */
cr107:
%n,n
	F?
	sra	R,1

/* x - &name */
cr130:
%n,n
	F
	li	R1,A2
	s	R1,R

%nl,n
