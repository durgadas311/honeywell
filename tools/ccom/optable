	I	A2,#1(R) //opt.185
	Q

%nb*,n
%nub*,n
	F*
	lca	&(x1),(R) //opt.191
	I	A2,(R) //opt.192
	lca	R,&(x1) //opt.193
	Q

/* - unary */
