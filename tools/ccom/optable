%{
/*
 * c code tables -- compile for side effects.
 * Also set condition codes properly (except for ++, --)
 */

struct table efftab[] = {
	{INCBEF,ci70},	/* prefix ++ */
	{DECBEF,ci70},	/* prefix -- */
	{INCAFT,ci70},	/* postfix ++ */
	{DECAFT,ci70},	/* postfix -- */
	{ASSIGN,ci80},	/* = */
	{ASPLUS,ci70},	/* += */
	{ASMINUS,ci70},	/* -= */
	{ASOR,ci78},	/* |= */
	{FSELA,ci16},	/* field assign, FSELA */
	{STRSET,ci116},	/* structure assignment setup */
	{0}
};
%}

/* = */
