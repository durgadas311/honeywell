	sla	R,8
	sra	R,8

/* / and % where divisor is unsigned or known to be positive */
cr117:
%n,c
	F
	T
	li	r0,A2
	I	r0,R-

%n,aw
	F
	T
	I	A2,R-

%n,ew*
